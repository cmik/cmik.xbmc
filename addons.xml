<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons> 
    <addon id="plugin.video.tfctv" name="TFC.tv" version="0.0.67" provider-name="cmik">
        <requires>
            <import addon="xbmc.python" version="2.1.0"/>
            <import addon="script.module.parsedom" version="0.9.1"/>
            <import addon="script.common.plugin.cache" version="2.5.5"/>
        </requires>
        <extension point="xbmc.python.pluginsource" library="default.py">
            <provides>video</provides>
        </extension>
      <extension point="xbmc.addon.metadata">
        <summary lang="en">TFC Pinoy TV</summary>
        <description lang="en">Enjoy TFC's Pinoy TV shows, movies, news and live channels! This is a non official TFC.tv addon. A subscription to TFC.tv website (http://tfc.tv) is required to enjoy TFC's catalog.</description>
        <disclaimer lang="en">No warranty here</disclaimer>
        <language>en</language>
        <platform>all</platform>
        <licence>GNU</licence>
      </extension>
    </addon>
    <addon id="repository.cmik.xbmc.addons" name="Cmik Repo" version="0.0.1" provider-name="cmik">
        <extension point="xbmc.addon.repository" name="Cmik Repo">
            <info compressed="false">http://github.com/cmik/cmik.xbmc/raw/master/addons.xml</info>
            <checksum>http://github.com/cmik/cmik.xbmc/raw/master/addons.xml.md5</checksum>
            <datadir zip="true">http://github.com/cmik/cmik.xbmc/raw/master/downloads</datadir>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary lang="en">Cmik XBMC Kodi Plugins</summary>
            <description lang="en">Install XBMC Kodi Plugins</description>
            <disclaimer lang="en">No warranty here</disclaimer>
            <language>en</language>
            <platform>all</platform>
            <licence>GNU</licence>
        </extension>
    </addon>
</addons>